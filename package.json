{
  "name": "eslint-plugin-wordpress",
  "version": "0.1.0",
  "description": "A collection of custom ESLint rules that help enforce JavaScript coding standard in the WordPress project.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ntwb/eslint-plugin-wordpress.git"
  },
  "homepage": "https://github.com/ntwb/eslint-plugin-wordpress#readme",
  "author": {
    "name": "Stephen Edgar",
    "email": "stephen@netweb.com.au",
    "url": "https://netweb.com.au"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "pretest": "npm run lint",
    "test": "nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "code",
    "code style",
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslintplugin",
    "eslint-plugin",
    "eslint-plugin-wordpress",
    "JavaScript",
    "jscs",
    "jshint",
    "jslint",
    "lint",
    "linter",
    "linting",
    "standard",
    "validate",
    "wordpress"
  ],
  "dependencies": {
    "eslint-plugin-ava": "~4.2.0",
    "eslint-plugin-backbone": "~2.0.2",
    "eslint-plugin-i18n": "~1.1.0",
    "eslint-plugin-jsdoc": "~3.0.2",
    "eslint-plugin-node": "~4.2.2",
    "eslint-plugin-qunit": "~3.0.0",
    "eslint-plugin-wpcalypso": "~3.2.0",
    "merge": "~1.2.0"
  },
  "devDependencies": {
    "ava": "~0.19.1",
    "coveralls": "~2.13.1",
    "eslint": "~4.1.0",
    "eslint-plugin-wordpress": "file:.",
    "is-plain-obj": "~1.1.0",
    "nyc": "~10.3.0"
  },
  "peerDependencies": {
    "eslint": ">=2.9.0"
  },
  "eslintConfig": {
    "plugins": [
      "ava"
    ],
    "extends": [
      "plugin:wordpress/recommended",
      "plugin:ava/recommended"
    ]
  }
}
